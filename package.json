  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StartPolymer/gulp-html-postcss/issues"
  },
  "homepage": "https://github.com/StartPolymer/gulp-html-postcss",
  "dependencies": {
    "bufferstreams": "^1.1.3",
    "gulp-util": "^3.0.8",
    "postcss-html": "^0.12.0",
    "postcss-load-config": "^1.2.0",
    "postcss-load-options": "^1.2.0",
    "postcss-load-plugins": "^2.3.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "peerDependencies": {
    "postcss": ">=6.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "from2-array": "0.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "postcss": "^6.0.15",
    "postcss-less": "^1.1.3",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  }
